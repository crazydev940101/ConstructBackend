{
  "name": "airdoc.pro",
  "version": "1.0.0",
  "description": "Airdoc projcet",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "postbuild": "cp -r src/sqlz/migrations build/sqlz/migrations && cp -r src/sqlz/seeders build/sqlz/seeders && cp -r src/views build/views",
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "migrate": "sequelize db:migrate",
    "create:db": "sequelize db:create",
    "make:migration": "sequelize migration:generate --name",
    "undo:migrate": "sequelize db:migrate:undo",
    "undo:migrate:all": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "make:seeder": "sequelize seed:generate --name",
    "undo:seeder": "sequelize db:seed:undo",
    "undo:seeder:all": "sequelize db:seed:undo:all",
    "lint:init": "tslint --init",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --project tsconfig.json --fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.8",
    "@types/express-boom": "^3.0.0",
    "@types/express-fileupload": "^1.4.1",
    "@types/express-session": "^1.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.17.7",
    "@types/passport": "^1.0.12",
    "@types/passport-azure-ad": "^4.3.1",
    "@types/passport-google-oauth": "^1.0.42",
    "@types/passport-jwt": "^3.0.8",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.6.3",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.6.0",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@azure/ai-form-recognizer": "^4.0.0",
    "@azure/search-documents": "^11.3.1",
    "@azure/storage-blob": "^12.14.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/ejs": "^3.1.2",
    "@types/pdf-parse": "^1.1.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.9",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.9",
    "express": "^4.17.1",
    "express-boom": "^3.0.0",
    "express-fileupload": "^1.4.0",
    "express-session": "^1.17.3",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.11",
    "openai": "^3.3.0",
    "passport": "^0.6.0",
    "passport-azure-ad": "^4.3.5",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.5.1",
    "sequelize": "^6.5.0",
    "sequelize-replace-enum-postgres": "^1.6.0",
    "stripe": "^12.9.0",
    "swagger-ui-express": "^4.1.4",
    "tsoa": "^3.8.0",
    "xlsx": "^0.18.5"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  }
}
